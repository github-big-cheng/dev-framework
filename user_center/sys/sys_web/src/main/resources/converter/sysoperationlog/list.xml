<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE trans SYSTEM "converter.dtd">
<trans name="sysOperationLog/list" desc="列表" group="操作日志" groupCode="sysOperationLog">
    <send>
        <!--这里存放的是参数,之后补充完善-->
        <item name="pageNo" desc="页码" type="Integer" default="1"/>
        <item name="pageSize" desc="页容量" type="Integer" default="15"/>
        <item name="timeQueryGe" desc="开始时间"/>
        <item name="timeQueryLe" desc="结束时间"/>
    </send>
    <rcv>
        <item name="total" type="Integer" desc="id" default="0"/>
        <item-list name="list" desc="列表">
            <item name="id" desc="id" type="Integer"/>
            <item name="billNo" desc="业务单号"/>
            <item name="bizId" desc="业务ID"/>
            <item name="userId" desc="用户ID"/>
            <item name="account" desc="账号"/>
            <item name="ip" desc="ip"/>
            <item name="machineName" desc="机器名称"/>
            <item name="opTime" desc="操作时间"/>
            <item name="createBy" desc="录入人" type="Integer"/>
            <item name="createByName" desc="创建人姓名" mapper="USER_PERSON_NAME_HASH" target="createBy"/>
            <item name="createTime" desc="录入时间"/>
            <item name="updateBy" desc="修改人" type="Integer"/>
            <item name="updateTime" desc="修改时间"/>
            <item name="updateByName" desc="修改人姓名" mapper="USER_PERSON_NAME_HASH" target="updateBy"/>
        </item-list>
    </rcv>
</trans>
